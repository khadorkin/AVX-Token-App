  "main": "src/electron/index.js",
  "scripts": {
    "dev": "electron-webpack dev",
    "server:dev": "webpack --watch --progress --config webpack/server.config.dev.node",
    "web-ui:dev": "webpack-dev-server --hot --watch --progress --config webpack/mobile-ui.config",
    "web:dev": "cross-env API_SERVER=webworker PROXY_SERVER=http://localhost:3001 yarn web-ui:dev",
    "webworker:dev": "webpack-dev-server --hot --watch --progress --config webpack/server.config.webworker",
    "electron:dev": "cross-env ELECTRON_WEBPACK_MAIN_SERVER=false API_SERVER=ws://localhost:5279 electron-webpack dev",
    "native:dev": "node node_modules/react-native/local-cli/cli.js start --port 8081 --verbose",
    "native:dev7": "cross-env BABEL_VERSION=7 node node_modules/react-native/local-cli/cli.js start --port 8081 --verbose",
    "native:cli": "node node_modules/react-native/local-cli/cli.js",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-sim": "react-native run-ios --simulator 'iPhone 6'",
    "electron:compile": "electron-webpack && yarn electron:extract-langs",
    "electron:build": "yarn electron:compile && electron-builder build",
    "electron:extract-langs": "node build/extractLocals.js",
    "build:dir": "yarn build -- --dir -c.compression=store -c.mac.identity=null",
    "postinstallX": "electron-builder install-app-deps",
    "lint": "eslint 'src/{common,electron,mobile-ui}/**/*.{js,jsx}' 'webpack/**/*.js' --fix",
    "format": "prettier 'src/**/*.{js,jsx,css,json}' --write",
    "vendor": "webpack --config webpack/mobile-vendor.config.js"
  },
  "keywords": [
    "avx",
    "token"
  ],
