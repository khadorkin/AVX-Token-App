[ignore]
.*/node_modules/**
.*/src/ui/**

[include]

[libs]
flow-typed

[lints]

[options]
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
module.name_mapper='^constants\(.*\)$' -> '<PROJECT_ROOT>/src/mobile-ui/constants\1'
module.name_mapper='^utils\(.*\)$' -> '<PROJECT_ROOT>/src/mobile-ui/utils\1'
module.name_mapper='^redux\(.*\)$' -> '<PROJECT_ROOT>/src/mobile-ui/redux\1'
module.name_mapper='^types\(.*\)$' -> '<PROJECT_ROOT>/src/mobile-ui/types\1'
module.name_mapper='^components\(.*\)$' -> '<PROJECT_ROOT>/src/mobile-ui/components\1'
module.name_mapper='^page\(.*\)$' -> '<PROJECT_ROOT>/src/mobile-ui/page\1'
module.name_mapper='^lbry\(.*\)$' -> '<PROJECT_ROOT>/src/ui/lbry\1'

[strict]
