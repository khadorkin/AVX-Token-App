<body class="w3-text-light-blue">
<can-import from="avx-token-app/styles.less" />
<can-import from="avx-token-app/app" export-as="viewModel" />

<!-- Navbar -->
<div class="w3-top w3-border-bottom w3-border-light-blue">
  <div class="w3-bar w3-black w3-card">
    <a class="w3-bar-item w3-button w3-hide-medium w3-hide-large w3-right" href="javascript:void(0)" onclick="myFunction()" title="Toggle Navigation Menu"><i class="fa fa-bars"></i></a>
    <a href="#" class="w3-bar-item w3-button w3-hover-text-light-blue w3-hover-black">AVX</a>
    <a href="#" class="w3-bar-item w3-button w3-hide-small w3-hover-text-light-blue w3-hover-black"><i class="fa fa-fire" aria-hidden="true"></i> Trending</a>
    <a href="#" class="w3-bar-item w3-button w3-hide-small w3-hover-text-light-blue w3-hover-black"><i class="fa fa-list" aria-hidden="true"></i> Subscription</a>
    <a href="#" class="w3-bar-item w3-button w3-hide-small w3-hover-text-light-blue w3-hover-black"><i class="fa fa-user-circle" aria-hidden="true"></i> Account</a>
    <a href="#" class="w3-bar-item w3-button w3-right w3-hide-small w3-hover-text-light-blue w3-hover-black"><i class="fa fa-search"></i></a>
    <input type="text" class="w3-bar-item w3-input w3-right w3-hide-small" placeholder="Search..">
  </div>
</div>

<!-- Navbar on small screens -->
<div id="navDemo" class="w3-bar-block w3-black w3-hide w3-hide-large w3-hide-medium w3-top w3-border-bottom w3-border-light-blue" style="margin-top:46px">
  <a href="#" class="w3-bar-item w3-button w3-padding-large">AVX</a>
  <a href="#" class="w3-bar-item w3-button w3-padding-large">Subscription</a>
  <a href="#" class="w3-bar-item w3-button w3-padding-large">Account</a>
</div>

